{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/strict": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "-ms-filter",
          "-webkit-tap-highlight-color",
          "-webkit-overflow-scrolling"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 14.8",
    "not and_ff <= 107",
    "not firefox <= 109",
    "not ios_saf <= 16.3",
    "not safari <= 16.3"
  ]
}