---
import HeaderSection from "@components/HeaderSection.astro";
import Footer from "@components/Footer.astro";
export interface Props {
  title: string;
}

const { title } = Astro.props;

import "../assets/css/bootstrap.min.css";
import "../assets/css/font-awesome.min.css";
import "../assets/css/animate.min.css";
import "../assets/css/magnific-popup.css";
import "../assets/css/slick.css";
import "../assets/css/default.css";
import "../assets/css/style.css";
---

<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link
      rel="icon"
      type="image/svg+xml"
      href="src/assets/images/icon/awrosoft-icon.png"
    />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@48,400,0,0"
    />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>

    <!--====== jquery js ======-->
  </head>
  <body>
    <HeaderSection />
    <slot />
    <Footer />

    <script is:inline src="src/assets/js/vendor/modernizr-3.6.0.min.js"
    ></script>
    <script is:inline src="src/assets/js/vendor/jquery-1.12.4.min.js"></script>
    <!--====== Bootstrap js ======-->
    <script is:inline src="src/assets/js/bootstrap.min.js"></script>

    <!--====== magnific popup js == ====-->
    <script is:inline src="src/assets/js/jquery.magnific-popup.min.js"></script>

    <!--====== counterup js == ====-->
    <script is:inline src="src/assets/js/jquery.counterup.min.js"></script>
    <script is:inline src="src/assets/js/waypoints.min.js"></script>

    <!--====== wow js ======-->
    <script is:inline src="src/assets/js/wow.js"></script>

    <!--====== circle progress js ======-->
    <script is:inline src="src/assets/js/circle-progress.js"></script>

    <!--====== Slick js ======-->
    <script is:inline src="src\assets\js\slick.min.js"></script>

    <!--====== Main js ======-->
    <script is:inline src="src/assets/js/main.js"></script>
  </body>
</html>
